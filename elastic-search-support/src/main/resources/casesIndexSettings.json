{
  "analysis":{
    "analyzer":{
      "phone_number_analyzer":{
        "type":"custom",
        "tokenizer":"keyword",
        "filter":[
          "whitespace_remove"
        ]
      },
      "case_id_analyzer":{
        "type":"custom",
        "tokenizer":"keyword",
        "filter":[
          "hyphen_remove"
        ]
      }
    },
    "filter":{
      "whitespace_remove":{
        "type":"pattern_replace",
        "pattern":" ",
        "replacement":""
      },
      "hyphen_remove":{
        "type":"pattern_replace",
        "pattern":"-",
        "replacement":""
      }
    }
  }
}
